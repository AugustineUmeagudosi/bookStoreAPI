{
  "name": "bookStore-api",
  "version": "1.0.0",
  "description": "The backend APIs for the bookStore app",
  "author": "Augustine Umeagudosi",
  "license": "UNLICENSED",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "19.9.0",
    "npm": "9.6.3"
  },
  "scripts": {
    "preset": "",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --require babel-core/register \"./test/**/*.test.js\" --timeout 100000 --exit",
    "start": "gulp serve",
    "lint": "eslint --fix app/ --ext .js",
    "prettify": "prettier --write --config .prettierrc \"**/*.{js,handlebars,md}\"",
    "prestart": "npm run migrate:up",
    "pretest": "npm run lint && cross-env NODE_ENV=test npm run migrate:reset && cross-env NODE_ENV=test npm run migrate:up && cross-env NODE_ENV=test npm run preset",
    "precommit": "npm run lint",
    "prepush": "npm run docker:build && npm run docker:test",
    "docker:build": "docker build -f docker/Dockerfile -t bookStore/${npm_package_name} .",
    "docker:start": "docker-compose -f docker/docker-compose.dev.yml up -d ${npm_package_name}",
    "docker:stop": "docker-compose -f docker/docker-compose.dev.yml down",
    "docker:test": "docker-compose -p ${npm_package_name}-test -f docker/docker-compose.dev.yml -f docker/docker-compose.test.yml run --rm ${npm_package_name} npm test",
    "docker:test:stop": "docker-compose -p ${npm_package_name}-test -f docker/docker-compose.dev.yml -f docker/docker-compose.test.yml down",
    "docker:watch": "docker-compose -f docker/docker-compose.dev.yml -f docker/docker-compose.watch.yml up ${npm_package_name} || true",
    "docker:shell": "docker-compose -f docker/docker-compose.dev.yml -f docker/docker-compose.watch.yml run ${npm_package_name} bash || true",
    "docker:tail": "docker-compose -f docker/docker-compose.dev.yml logs -f",
    "prepare": "husky install",
    "migrate": "db-migrate --migrations-dir ./app/db/migrations",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:reset": "npm run migrate reset",
    "migrate:create": "npm run migrate create",
    "seed:create": "npm run migrate create:seeds",
    "seed:up": "npm run migrate up:seeds",
    "seed:down": "npm run migrate down:seeds",
    "seed:reset": "npm run migrate reset:seeds"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "chai-http": "^4.3.0",
    "chance": "^1.1.10",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "debug": "^2.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.14.0",
    "express-async-errors": "^3.1.1",
    "faker": "^5.5.3",
    "file-stream-rotator": "0.0.7",
    "helmet": "^3.3.0",
    "http-status": "^1.6.2",
    "husky": "^9.0.10",
    "ioredis": "^5.3.2",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.7.0",
    "multer": "^1.4.3",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.1.0",
    "pg-monitor": "^2.0.0",
    "pg-promise": "^11.3.0",
    "uuid": "^9.0.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^4.3.4",
    "del": "^2.2.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^6.1.2",
    "gulp-jshint-jenkins-reporter": "^1.1.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-mocha": "^7.0.2",
    "gulp-newer": "^1.3.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.8",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "supertest": "^6.0.1"
  }
}
